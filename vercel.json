{
    "rewrites": [
        {
            "source": "/rank/:id",
            "destination": "/api?c=:id"
        },
        {
            "source": "/locations",
            "destination": "api/v2/locations"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}